<%- model_class = SurveyValueSet -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:value1) %></th>
      <th><%= model_class.human_attribute_name(:value2) %></th>
      <th><%= model_class.human_attribute_name(:value3) %></th>
      <th><%= model_class.human_attribute_name(:value4) %></th>
      <th><%= model_class.human_attribute_name(:value5) %></th>
      <th><%= model_class.human_attribute_name(:value6) %></th>
      <th><%= model_class.human_attribute_name(:value7) %></th>
      <th><%= model_class.human_attribute_name(:survey_data_set_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @survey_value_sets.each do |survey_value_set| %>
      <tr>
        <td><%= link_to survey_value_set.id, survey_value_set_path(survey_value_set) %></td>
        <td><%= survey_value_set.value1 %></td>
        <td><%= survey_value_set.value2 %></td>
        <td><%= survey_value_set.value3 %></td>
        <td><%= survey_value_set.value4 %></td>
        <td><%= survey_value_set.value5 %></td>
        <td><%= survey_value_set.value6 %></td>
        <td><%= survey_value_set.value7 %></td>
        <td><%= survey_value_set.survey_data_set_id %></td>
        <td><%=l survey_value_set.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_survey_value_set_path(survey_value_set), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      survey_value_set_path(survey_value_set),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_survey_value_set_path,
            :class => 'btn btn-primary' %>
