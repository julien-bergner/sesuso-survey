<h1>Listing survey_locations</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Notice</th>
    <th>Street</th>
    <th>Street number</th>
    <th>Zip</th>
    <th>City</th>
    <th>Url</th>
    <th>Map url</th>
    <th>Survey area</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @survey_locations.each do |survey_location| %>
  <tr>
    <td><%= survey_location.title %></td>
    <td><%= survey_location.description %></td>
    <td><%= survey_location.notice %></td>
    <td><%= survey_location.street %></td>
    <td><%= survey_location.street_number %></td>
    <td><%= survey_location.zip %></td>
    <td><%= survey_location.city %></td>
    <td><%= survey_location.url %></td>
    <td><%= survey_location.map_url %></td>
    <td><%= survey_location.survey_area_id %></td>
    <td><%= link_to 'Show', survey_location %></td>
    <td><%= link_to 'Edit', edit_survey_location_path(survey_location) %></td>
    <td><%= link_to 'Destroy', survey_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Survey location', new_survey_location_path %>
