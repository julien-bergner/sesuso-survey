<%- model_class = SurveyDataSet -%>
<div class="page-header">
  <h1><%= "#{t '.title', :default => model_class.model_name.human} #{@survey_data_set.id}" %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:start) %>:</strong></dt>
  <dd><%= @survey_data_set.start %></dd>
  <dt><strong><%= model_class.human_attribute_name(:end) %>:</strong></dt>
  <dd><%= @survey_data_set.end %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @survey_data_set.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:survey_location_id) %>:</strong></dt>
  <dd><%= @survey_data_set.survey_location_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              :back, :class => 'btn'  %>
  <%= if can? :edit, @survey_data_set then link_to t('.edit', :default => t("helpers.links.edit")),
              edit_survey_data_set_path(@survey_data_set), :class => 'btn' end %>
  <%= if can? :destroy, @survey_data_set then link_to t('.destroy', :default => t("helpers.links.destroy")),
              survey_data_set_path(@survey_data_set),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' end %>
</div>
