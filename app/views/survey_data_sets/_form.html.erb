<%= simple_form_for @survey_data_set, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :start %>
  <%= f.input :end %>
  <%= f.input :user_id %>
  <%= f.input :survey_location_id %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                survey_data_sets_path, :class => 'btn' %>
  </div>
<% end %>
